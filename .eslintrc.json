{
  "env": {
    "browser": true,
    "jasmine": true,
    "protractor": true,
    "phantomjs": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true,
    "squel": true,
    "StatusBar": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
